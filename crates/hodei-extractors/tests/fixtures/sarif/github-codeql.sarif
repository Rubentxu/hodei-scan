{
  "version": "2.1.0",
  "$schema": "https://json.schemastore.org/sarif-2.1.0.json",
  "runs": [
    {
      "tool": {
        "driver": {
          "name": "CodeQL",
          "version": "2.14.0",
          "rules": [
            {
              "id": "py/sql-injection",
              "name": "SQL injection",
              "shortDescription": {
                "text": "Possible SQL injection vulnerability"
              },
              "fullDescription": {
                "text": "String concatenation or interpolation used to construct SQL query"
              }
            }
          ]
        }
      },
      "results": [
        {
          "ruleId": "py/sql-injection",
          "ruleIndex": 0,
          "level": "error",
          "message": {
            "text": "This use of \"query += ...\" can cause SQL injection because the concatenated string is later passed to a database execution function."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/users.py",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 42,
                  "startColumn": 12,
                  "endLine": 42,
                  "endColumn": 53
                }
              }
            }
          ],
          "properties": {
            "security-severity": 8.1,
            "cwe": ["CWE-89"],
            "tags": ["security", "external/cwe/cwe-089"]
          }
        },
        {
          "ruleId": "py/sql-injection",
          "ruleIndex": 0,
          "level": "warning",
          "message": {
            "text": "This use of f-string can cause SQL injection because the interpolated variable is later passed to a database execution function."
          },
          "locations": [
            {
              "physicalLocation": {
                "artifactLocation": {
                  "uri": "src/users.py",
                  "uriBaseId": "%SRCROOT%"
                },
                "region": {
                  "startLine": 55,
                  "startColumn": 8,
                  "endLine": 55,
                  "endColumn": 35
                }
              }
            }
          ],
          "properties": {
            "security-severity": 7.2,
            "cwe": ["CWE-89"],
            "tags": ["security", "external/cwe/cwe-089"]
          }
        }
      ]
    }
  ]
}
